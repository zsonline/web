{
  "require": {
    "craftcms/ckeditor": "3.4.0",
    "craftcms/cms": "4.4.17",
    "nystudio107/craft-plugin-vite": "^4.0",
    "nystudio107/craft-vite": "4.0.6",
    "putyourlightson/craft-blitz": "4.5.1",
    "putyourlightson/craft-sprig": "2.6.2",
    "spicyweb/craft-embedded-assets": "3.1.6",
    "twig/html-extra": "^3.4",
    "verbb/comments": "2.0.7",
    "verbb/navigation": "2.0.20",
    "verbb/super-table": "3.0.9",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "editor\\": "modules/editor/src/",
      "site\\": "modules/site/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.0"
    }
  }
}
