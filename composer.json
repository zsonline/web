{
  "require": {
    "craftcms/ckeditor": "3.8.3",
    "craftcms/cms": "4.9.4",
    "nystudio107/craft-plugin-vite": "^4.0",
    "nystudio107/craft-vite": "4.0.9",
    "putyourlightson/craft-sprig": "2.8.1",
    "spicyweb/craft-embedded-assets": "4.1.0",
    "twig/html-extra": "^3.4",
    "verbb/comments": "2.0.12",
    "verbb/navigation": "2.0.27",
    "verbb/super-table": "3.0.14",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "editor\\": "modules/editor/src/",
      "site\\": "modules/site/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.0"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}